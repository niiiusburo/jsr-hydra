# JSR Hydra Trading System - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Database
DB_USER=postgres
DB_PASS=changeme
DATABASE_URL=postgresql+asyncpg://${DB_USER}:${DB_PASS}@jsr-postgres:5432/jsr_hydra

# Security (MUST change in production)
JWT_SECRET=generate-with-openssl-rand-hex-32
API_KEY=generate-with-openssl-rand-hex-32
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme

# MT5
MT5_LOGIN=0
MT5_PASSWORD=
MT5_SERVER=

# Redis
REDIS_URL=redis://jsr-redis:6379/0

# Trading
DRY_RUN=true
APP_ENV=development

# OpenAI (optional)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1/chat/completions

# Z.AI (optional)
ZAI_API_KEY=
ZAI_MODEL=glm-4.6
ZAI_BASE_URL=https://api.z.ai/api/paas/v4/chat/completions

# Brain LLM runtime default provider (openai | zai)
BRAIN_LLM_PROVIDER=openai
